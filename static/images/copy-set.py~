
import os
import sys

imageset = sys.argv[1]

for i in range(8):
    base = 'pro-set-' imageset + '-part-0'
    os.system('cp ' + base + '.svg --export-png=' + base + '.png')
    os.system('inkscape ' + base + '-failed.svg --export-png=' + base + '-failed.png')
    os.system('inkscape ' + base + '-icon.svg --export-png=' + base + '-icon.png')    
